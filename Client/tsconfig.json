{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "esnext",
    "jsx": "react",
    "sourceMap": true,
    "noEmit": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks":false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "src",
    "fonts.d.ts"
  ]
}
